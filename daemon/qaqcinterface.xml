<service>
	<id>qaqcinterface.exe</id>
	<name>qaqcinterface</name>
	<description>Air Quality web interface</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\jteng.admin\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\WebApps\qaqcinterface\app.js</argument>
	<argument>--log</argument>
	<argument>qaqcinterface wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>C:\WebApps\qaqcinterface</workingdirectory>
</service>